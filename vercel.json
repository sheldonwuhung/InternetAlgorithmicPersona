{
  "version": 2,
  "functions": [
    {
        "app/api/**/*.js": {
            "includeFiles": "node_modules/@google/genai/**"
         }
    }
    ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}